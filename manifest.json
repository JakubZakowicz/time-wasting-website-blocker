{
  "manifest_version": 2,
  "name": "Website Analyzer",
  "version": "1.0",
  "description": "Analyzes visited websites to determine if they are beneficial or time-wasting",
  "permissions": [
    "tabs",
    "webNavigation",
    "storage",
    "notifications",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup/popup.html"
  }
}
